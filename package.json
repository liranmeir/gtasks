{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gtasks",
  "title": "gtasks",
  "description": "",
  "icon": "command-icon.png",
  "author": "liranmeir",
  "license": "MIT",
  "commands": [
    {
      "name": "today",
      "title": "Today Tasks",
      "subtitle": "Gtasks",
      "description": "See your Gtasks tasks due today.",
      "mode": "view",
      "preferences": [
        {
          "name": "todayGroupBy",
          "type": "dropdown",
          "data": [
            {
              "title": "Default",
              "value": "default"
            },
            {
              "title": "Project",
              "value": "project"
            },
            {
              "title": "Priority",
              "value": "priority"
            },
            {
              "title": "Label",
              "value": "label"
            }
          ],
          "default": "default",
          "required": false,
          "title": "Today's Group By Options",
          "description": "Indicates how you want to group your tasks in the Today view."
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.40.3",
    "@raycast/utils": "^1.4.9"
  },
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
